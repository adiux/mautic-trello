    "main": "Gruntfile.js",
    "author": "Idea-2",
    "devDependencies": {
      "@openapitools/openapi-generator-cli": "0.0.19-4.1.2",
      "chai": "^4.2.0",
      "chai-http": "^4.3.0",
      "chai-openapi-response-validator": "^0.2.5",
      "grunt": "~0.4.5",
      "grunt-wp-i18n": "~0.5.0",
      "grunt-wp-readme-to-markdown": "~1.0.0",
      "mocha": "^6.2.2",
      "querystring": "^0.2.0"
    },
    "scripts": {
      "test": "NODE_PATH=. mocha tests",
      "generate-api-trello": "openapi-generator generate -i docs/api/i2-trello.oas3.yml -g php -o Openapi --additional-properties='invokerPackage=MauticPlugin\\\\Idea2TrelloBundle\\\\Openapi\\\\lib,bundleName=Idea2Trello,phpLegacySupport=false,variableNamingConvention=camelCase'",
      "openapi-generator-help": "openapi-generator config-help -g php-symfony",
      "format-all-code": "./vendor/bin/php-cs-fixer fix . --rules=@Symfony",
      "update-autoload": "./composer.phar dump-autoload -o"
    }
  }
  